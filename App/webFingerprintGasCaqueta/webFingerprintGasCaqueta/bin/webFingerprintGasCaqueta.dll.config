<configuration>
  <configSections>
    <section name="extnet" type="Ext.Net.GlobalConfig" requirePermission="false" />
  </configSections>
  <connectionStrings>
     <add name="ParametroContext" connectionString="metadata=res://*/Models.Parametro.Model.csdl|res://*/Models.Parametro.Model.ssdl|res://*/Models.Parametro.Model.msl;provider=MySql.Data.MySqlClient;provider connection string=&quot;server=192.168.0.100;user id=planta;password=planta123;persistsecurityinfo=True;database=parametro&quot;" providerName="System.Data.EntityClient" />
    <!--<add name="ControlPersonal" connectionString="Data Source=192.168.0.100; database=control_acceso; uid=planta; password=planta123" providerName="MySql.Data.MySqlClient.MySqlConnection" />-->
    <!--<add name="ControlPersonal" connectionString="Data Source=localhost; database=control_acceso; uid=root; password=root" providerName="MySql.Data.MySqlClient.MySqlConnection" />-->
    <add name="ControlPersonal" connectionString="Data Source=localhost; database=control_acceso; uid=root; password=mysql789" providerName="MySql.Data.MySqlClient.MySqlConnection" />
    <!--<add name="ControlPersonal" connectionString="Data Source=192.168.0.91; database=control_acceso; uid=planta; password=planta123" providerName="MySql.Data.MySqlClient.MySqlConnection" />-->
